{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": [".next/**", "!.next/cache/**"],
			"dotEnv": [".env"]
		},
		"lint": {},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"test": {
			"cache": false,
			"persistent": true
		}
	}
}
